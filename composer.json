{
    "name": "depthbomb/csrf-bundle",
    "description": "Simple CSRF protection for Symfony routes",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "depthbomb"
        }
    ],
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "symfony/config": "6.3.*",
        "symfony/dependency-injection": "6.3.*",
        "symfony/event-dispatcher": "6.3.*",
        "symfony/http-foundation": "6.3.*",
        "symfony/http-kernel": "6.3.*",
        "symfony/security-csrf": "6.3.*"
    },
    "autoload": {
        "psr-4": {
            "Depthbomb\\CsrfBundle\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
